{
  "name": "@taro-plus/ant-design-taro-monorepo",
  "private": true,
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "build:docs": "dumi build",
    "build:lib": "tsx scripts/lib.build.ts",
    "clean": "rimraf ./**/dist ./**/node_modules",
    "dev:docs": "dumi dev",
    "dev:h5": "pnpm --filter antd-taro-demo dev:h5",
    "dev:lib": "tsx scripts/lib.dev.ts",
    "dev:weapp": "pnpm --filter antd-taro-demo dev:weapp",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "dependencies": {
    "dumi": "^2.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "chokidar": "^3.5.3",
    "del": "^7.0.0",
    "esbuild": "^0.16.7",
    "eslint": "^8.29.0",
    "eslint-config-alloy": "^4.7.0",
    "eslint-plugin-react": "^7.31.11",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "rimraf": "^3.0.2",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4"
  },
  "packageManager": "pnpm@7.18.2",
  "engines": {
    "node": "16.x",
    "pnpm": "7.x"
  }
}
