{
  "name": "antd-taro",
  "private": true,
  "description": "",
  "homepage": "https://adt.center",
  "bugs": {
    "url": "https://github.com/sushi-su/ant-design-taro/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sushi-su/ant-design-taro.git",
    "branch": "main",
    "platform": "github"
  },
  "license": "MIT",
  "author": {
    "name": "Sushi Su"
  },
  "main": "cjs/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "typings": "es/index.d.ts",
  "files": [
    "es",
    "cjs",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build:alipay": "taro build --type alipay",
    "build:docs": "dumi build",
    "build:h5": "taro build --type h5",
    "build:jd": "taro build --type jd",
    "build:lib": "gulp",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "build:rn": "taro build --type rn",
    "build:swan": "taro build --type swan",
    "build:tt": "taro build --type tt",
    "build:weapp": "cross-env NODE_ENV=production taro build --type weapp",
    "clean": "rimraf src/.umi src/.umi-production lib coverage dist docs-dist package-lock.json node_modules",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:docs": "dumi dev",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:weapp": "cross-env NODE_ENV=development taro build --type weapp --watch",
    "publish": "npm publish ./lib --registry=https://registry.npmjs.org",
    "publish:alpha": "npm publish ./lib --registry=https://registry.npmjs.org --tag alpha",
    "publish:dev": "npm publish ./lib --registry=https://registry.npmjs.org --tag dev",
    "test": "rimraf lib && jest",
    "test:coverage": "rimraf lib && jest --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@tarojs/components": "3.4.7",
    "@tarojs/plugin-framework-react": "3.4.7",
    "@tarojs/react": "3.4.7",
    "@tarojs/runtime": "3.4.7",
    "@tarojs/taro": "3.4.7",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@tarojs/mini-runner": "3.4.7",
    "@tarojs/webpack-runner": "3.4.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.1.1",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "antd-mobile-icons": "^0.2.2",
    "babel-preset-taro": "3.4.7",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "dumi": "^1.1.40",
    "eslint": "^8.13.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-plugin-react": "^7.29.4",
    "f2-touchemulator": "^0.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-less": "^5.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-packagejson": "^2.2.17",
    "qrcode.react": "^3.0.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "stylelint": "^14.7.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "through2": "^4.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "umi-hd": "^5.0.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
